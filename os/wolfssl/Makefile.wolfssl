CFLAGS += -DUSE_WOLFSSL -DWOLFSSL_USER_SETTINGS -DWOLFCRYPT_ONLY
CFLAGS += -I$(CONTIKI)/$(CONTIKI_NG_WOLFSSL_DIR)/wolfssl/

# WOLFSCRYPT_SRC_DIR = $(CONTIKI)/$(CONTIKI_NG_WOLFSSL_DIR)/wolfssl/wolfscrypt/src
WOLFSCRYPT_SRC_DIR = $(CONTIKI_NG_WOLFSSL_DIR)/wolfssl/wolfcrypt/src

MODULES_SOURCES_EXCLUDES += \
	evp.c \
	misc.c \
	ecc.c \
	sha512.c

MODULES += $(WOLFSCRYPT_SRC_DIR)

$(info CFLAGS: $(CFLAGS))
$(info MODULES: $(MODULES))